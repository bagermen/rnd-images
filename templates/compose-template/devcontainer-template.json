{
  "id": "compose-template",
  "version": "2.6.0",
  "name": "rnd environment",
  "description": "Environment of rnd images",
  "keywords": [
    "rnd"
  ],
  "options": {
    "os": {
      "type": "string",
      "description": "Specify OS based variant",
      "proposals": [
        "ubuntu"
      ],
      "default": ""
    },
    "db": {
      "type": "string",
      "description": "Specify database",
      "enum": [
        "postgres",
        "postgres16"
      ],
      "default": "postgres"
    }
  },
  "optionalPaths": [
    "CHANGELOG.md"
  ]
}
